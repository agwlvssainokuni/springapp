<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
	PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
	"http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<context id="common" targetRuntime="MyBatis3Simple">
		<plugin type="org.mybatis.generator.plugins.SerializablePlugin" />
		<plugin type="org.mybatis.generator.plugins.EqualsHashCodePlugin" />
		<commentGenerator>
			<property name="suppressDate" value="true" />
		</commentGenerator>
		<jdbcConnection driverClass="org.h2.Driver"
			connectionURL="jdbc:h2:./dbschema/build/mbgenerator" userId="sa" />
		<javaModelGenerator targetPackage="cherry.spring.common.db.gen.dto"
			targetProject="common/src/main/java">
			<property name="rootClass" value="cherry.spring.common.db.BaseDto" />
		</javaModelGenerator>
		<sqlMapGenerator targetPackage="cherry.spring.common.db.gen.mapper"
			targetProject="common/src/main/java" />
		<javaClientGenerator targetPackage="cherry.spring.common.db.gen.mapper"
			type="XMLMAPPER" targetProject="common/src/main/java" />
		<table tableName="users" />
		<table tableName="signup_requests" />
		<table tableName="mail_templates" />
		<table tableName="mail_template_addresses" />
		<table tableName="mail_template_texts" />
	</context>
</generatorConfiguration>
